class @class_name(@inherited_class_name):

    exclusion_list = ["exclusion_list", "_state", "__class__", "__delattr__", "__dict__", "__doc__", "__getattribute__", "__hash__", "__init", "__init__",
                      "__mapper__", "__mapper_args__", "__metaclass__", "__module__", "__new__", "__reduce__", "__reduce_ex__",
                      "__repr__", "__setattr__", "__str__", "__table__", "__tablename__", "__weakref__", "_class_state", "_decl_class_registry", 
                      "c", "get_parent", "get_parent_name", "parent_class", "metadata"]

    __tablename__ = "@table_name"

    @discriminator
    @mapper_args

    parent_class = @parent_class_name_none_other

@attribute_list

    def __init__(self):
        pass

    def get_parent(self):
        return @parent_class_name_same_other

    def get_parent_name(self):
        return self.get_parent().__name__
