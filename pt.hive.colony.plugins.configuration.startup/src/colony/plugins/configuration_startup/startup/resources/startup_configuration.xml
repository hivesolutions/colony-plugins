<startup_configuration xmlns="http://www.hive.pt"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.hive.pt xsd/startup_configuration.xsd">
    <plugins>
        <plugin>
            <name>Main Gui Plugin</name>
            <id>pt.hive.colony.plugins.main.gui</id>
            <version>1.0.0</version>
            <load>true</load>
        </plugin>
        <plugin>
            <name>Console Main Plugin</name>
            <id>pt.hive.colony.plugins.main.console</id>
            <version>1.0.0</version>
            <load>true</load>
        </plugin>
        <plugin>
            <name>Autoloader Plugin</name>
            <id>pt.hive.colony.plugins.misc.autoloader</id>
            <version>1.0.0</version>
            <load>true</load>
        </plugin>
        <plugin>
            <name>Document Pdf Plugin</name>
            <id>pt.hive.colony.plugins.document.pdf</id>
            <version>1.0.0</version>
            <load>false</load>
        </plugin>
    </plugins>
</startup_configuration>
