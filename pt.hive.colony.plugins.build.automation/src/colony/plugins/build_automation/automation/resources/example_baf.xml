<build_automation xmlns="http://www.hive.pt"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.hive.pt xsd/baf.xsd">
	<parent>
		<id></id>
		<version></version>
	</parent>
	<artifact>
		<id></id>
		<version></version>
		<type></type>
		<name></name>
		<description></description>
		<url></url>
	</artifact>
	<build>
		<default_stage></default_stage>
		<directory></directory>
		<output_directory></output_directory>
		<final_name></final_name>
		<source_directory></source_directory>
		<dependencies>
			<dependency>
				<id></id>
				<version></version>
				<run></run>
				<scope></scope>
			</dependency>
		</dependencies>
		<plugins>
			<plugin>
				<id></id>
				<version></version>
				<stage></stage>
				<configuration>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id></id>
			<activation>
				<property>
					<name></name>
					<value></value>
				</property>
			</activation>
			<build></build>
		</profile>
	</profiles>
</build_automation>
